	"description": "study react antd",
	"browserslist": [
		"ie > 7",
		"> 0.0001%"
	],
	"standard": {
		"parser": "babel-eslint",
		"globals": [
			"$",
			"moment",
			"numeral",
			"cookie",
			"PubSub",
			"Signal"
		],
		"ignore": [
			"dist",
			"build",
			"static",
			"node_modules"
		]
	},
	"scripts": {
		"dll": "webpack --config webpack.dev.dll.js",
		"dev": "webpack --config webpack.dev.config.js",
